{
  "name": "icp-query-extension",
  "displayName": "ICP Query Extension",
  "version": "1.0.7",
  "description": "Browser Extension for ICP Query",
  "author": "yuedanlabs",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "0.67.4",
    "stdf": "^0.2.5",
    "svelte": "3.58.0",
    "svelte-preprocess": "5.0.3"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.3",
    "@tailwindcss/forms": "^0.5.3",
    "@types/chrome": "0.0.227",
    "@types/node": "18.15.11",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "prettier": "2.8.7",
    "tailwindcss": "^3.3.2",
    "typescript": "5.0.3"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "storage"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "fonts/symbol.svg"
        ],
        "matches": [
          "https://*/*"
        ]
      }
    ]
